{
  "name": "simensen/message-tracing",
  "description": "Causation and Correlation tracing for Messages",
  "license": "MIT",
  "type": "lib",
  "require": {
    "php": ">=8.2"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.45",
    "phpunit/phpunit": "^11.2.8",
    "symfony/uid": "^6.4 || ^7.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Simensen\\MessageTracing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Simensen\\MessageTracing\\Tests\\Fixtures\\": "tests/Fixtures/",
      "Simensen\\MessageTracing\\Tests\\Unit\\": "tests/Unit/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    },
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.1.x-dev"
    },
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
